{
  "name": "docs-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs-ui/src",
  "tags": [],
  "targets": {
    "build": {
      "builder": "nx:run-commands",
      "outputs": ["dist/apps/docs-ui"],
      "options": {
        "command": "npx gulp --gulpfile apps/docs-ui/gulpfile.js bundle"
      }
    },
    "serve": {
      "builder": "nx:run-commands",
      "options": {
        "command": "npx gulp --gulpfile apps/docs-ui/gulpfile.js preview"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/docs-ui/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
